# Otimizações ultra-agressivas para builds rápidos (5 minutos máximo)
legacy-peer-deps=true
prefer-offline=true
audit=false
fund=false
progress=false
loglevel=error
save-exact=false
package-lock=false
prefer-dedupe=true
# Otimizações adicionais para reduzir tamanho e tempo
engine-strict=false
optional=false
shamefully-hoist=false
strict-peer-deps=false

