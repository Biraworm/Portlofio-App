{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main.ts"
    }
  ],
  "installCommand": "npm ci --legacy-peer-deps --prefer-offline --no-audit --silent --no-optional --no-fund --no-progress --ignore-scripts"
}